{
  "headers": [
    {
      "source": "/gallery/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow, noimageindex"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/gallery.json",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow, noimageindex"
        }
      ]
    }
  ]
} 